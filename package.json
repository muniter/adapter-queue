{
  "name": "@muniter/queue",
  "version": "1.0.0",
  "description": "A TypeScript queue system inspired by Yii2-Queue architecture",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:integration": "vitest run tests/integration",
    "test:unit": "vitest run tests --exclude tests/integration",
    "lint": "tsc --noEmit",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build && npm test",
    "queue:worker": "node dist/cli/worker.js",
    "queue:worker:isolate": "node dist/cli/worker.js --isolate"
  },
  "keywords": [
    "queue",
    "job",
    "worker",
    "sqs",
    "database",
    "typescript",
    "yii2-queue"
  ],
  "author": "",
  "license": "MIT",
  "files": [
    "dist/**/*",
    "src/**/*",
    "README.md",
    "package.json"
  ],
  "devDependencies": {
    "@aws-sdk/client-sqs": "^3.831.0",
    "@types/better-sqlite3": "7.6.13",
    "@types/node": "^20.0.0",
    "better-sqlite3": "^12.0.0",
    "redis": "^5.5.6",
    "testcontainers": "^11.0.3",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./sqlite": {
      "import": "./dist/adapters/sqlite.js",
      "types": "./dist/adapters/sqlite.d.ts"
    },
    "./redis": {
      "import": "./dist/adapters/redis.js",
      "types": "./dist/adapters/redis.d.ts"
    },
    "./sqs": {
      "import": "./dist/adapters/sqs.js",
      "types": "./dist/adapters/sqs.d.ts"
    }
  },
  "peerDependencies": {
    "@aws-sdk/client-sqs": "^3.0.0",
    "better-sqlite3": "^9.0.0",
    "redis": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "@aws-sdk/client-sqs": {
      "optional": true
    },
    "better-sqlite3": {
      "optional": true
    },
    "redis": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
